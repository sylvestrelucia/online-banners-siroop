<div>
    {% if modifier %}
        {% include '@a-svg-sprite--' ~ modifier %}
	{% endif %}
</div>

<div class="mat-svg">

{% for svg in data.svg %}
	<div class="mat-svg__item">
		<svg class="mat-svg__svg">
			<use xlink:href="#{{ svg }}"></use>
		</svg>

		<div class="mat-buttons__code">
			<code>
				{{ svg }}
			</code>
		</div>

	</div>

{% endfor  %}

</div>
