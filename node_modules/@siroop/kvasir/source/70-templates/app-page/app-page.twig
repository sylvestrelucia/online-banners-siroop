<div class="t-app-page" data-c-name="AppPage">
	<div class="t-app-page__content">
		<img class="t-app-page__left" src="{{ data.imageLeft }}" alt="{{ data.imageLeftAlt }}">
	</div>
	<div class="t-app-page__content t-app-page__banner">
		<div class="t-app-page__banner-logo">
			{% include '@a-svg-bonbon' with { data: { title: data.titleLogo }} %}
		</div>
		<div class="t-app-page__banner-text">{{ data.appBanner.text }}</div>
		<div class="t-app-page__banner-img-group">
			<a href="{{ data.appBanner.linkLeftStore }}" target="_blank">
				<img class="t-app-page__banner-img" src="{{ data.appBanner.storeLeft }}" alt="{{ data.appBanner.storeAltLeft }}">
			</a>
			<a href="{{ data.appBanner.linkRightStore }}" target="_blank">
				<img class="t-app-page__banner-img" src="{{ data.appBanner.storeRight }}" alt="{{ data.appBanner.storeAltRight }}">
			</a>
		</div>
		<span class="t-app-page__banner-text">by</span>
		<span class="t-app-page__banner-logo-bottom">
			{% include '@m-logo' with { modifier: 'white'} %}
		</span>
	</div>
	<div class="t-app-page__content">
		<img class="t-app-page__right" src="{{ data.imageRight }}" alt="{{ data.imageRightAlt }}">
	</div>
</div>
