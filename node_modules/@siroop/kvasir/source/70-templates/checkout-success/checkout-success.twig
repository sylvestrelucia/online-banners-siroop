<div class="t-checkout-success" data-c-name="CheckoutSuccess">
	<h1 class="t-checkout-success__title">
		{{ data.title }}
		{% include '@a-svg-icon' with {
			modifiers: ['stroked', 'large'],
			data: {
				name: 'checkmark'
			}
		} %}
	</h1>
	<div class="t-checkout-success__body">
		{{ data.body|raw }}

		<button class="t-checkout-success__button js-t-checkout-success__button">
			{{ data.buttonText }}
		</button>
	</div>
</div>
