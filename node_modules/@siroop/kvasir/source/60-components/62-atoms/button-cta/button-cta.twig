<button class="a-button-cta{% if modifier %} a-button-cta--{{ modifier }}{% endif %}  a-button-cta--icon-left">
	{{ data.text }}
	{% if data.icon %}
		<div class="a-button-cta__icon">
			{% include '@a-svg-icon' with {
				data: {
					name: data.icon
				},
				modifiers: ['stroked', 'top']
				}
			%}
		</div>
	{% endif %}
</button>

