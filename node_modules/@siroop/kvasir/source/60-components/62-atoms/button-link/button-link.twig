<a class="a-button-link{% if modifier %} a-button-link--{{ modifier }}{% endif %}" href="{{ data.url }}">
	<span class="a-button-link__title">
		{{ data.title }}
	</span>
	<span class="a-button-link__icon">
		{% include "@a-svg-icon" with {
			modifiers: ['small', 'stroked-thin'],
			data: {
				name: 'arrow-right'
			}
		} %}
	</span>
</a>
