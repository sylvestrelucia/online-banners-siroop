<div class="o-gallery js-o-gallery{% if modifier %} o-gallery--{{ modifier }}{% endif %}" data-c-name="Gallery">
	<div class="o-gallery__main-carousel js-o-gallery__main-carousel">
		<div class="o-gallery__items js-o-gallery__items" data-label-next="{{ data.labelNext }}" data-label-prev="{{ data.labelPrev }}">
			{% for item in data.items %}
				<div class="o-gallery__item">
					{% include '@m-image-responsive' with { data: item.image } %}
				</div>
			{% endfor %}
		</div>
	</div>
</div>
