<div class="o-footer-meta{% if modifier %} o-footer-meta--{{ modifier }}{% endif %}">
	<div class="o-footer__wrapper">
		<div class="o-footer-meta__inner">
			<div class="o-footer-meta__channels">
				<div class="o-footer-meta__channels-title">
					{{ data.labelSocialLinks }}
				</div>
				<div class="o-footer-meta__channels-items">
					{% include '@m-social-links' with { data: data.socialLinks } %}
				</div>
			</div>
			<div class="o-footer-meta__links">
				{% for link in data.links %}
					<a class="o-footer-meta__link" href="{{ link.url }}">{{ link.title }}</a>
				{% endfor %}
			</div>
		</div>
	</div>
</div>
