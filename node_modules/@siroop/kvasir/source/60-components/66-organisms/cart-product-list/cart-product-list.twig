<div class="o-cart-product-list{% if modifier %} o-cart-product-list--{{modifier}}{% endif %}" data-c-name="CartProductList">
	<ul class="o-cart-product-list__list">
		{% for product in data.products %}
		<li class="o-cart-product-list__item">
			{% include '@o-cart-product' with {
				data: {
						product: product,
						remove: data.remove,
						readOnly: data.readOnly,
					},
				modifier: '',
			} %}
		</li>
		{% endfor %}
	</ul>
</div>
