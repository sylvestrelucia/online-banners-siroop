<div class="o-form-create-account">
	<div class="o-form-create-account__fields obj-form__row obj-form__layout obj-form__layout-6">
		<div class="o-form-create-account__field obj-form__col">
			<label class="o-form-create-account__label o-form-create-account__label--required" for="{{ data.form.fields.password.id }}">{{ data.form.fields.password.label }}</label>
			<span class="o-form-create-account__sublabel">{{ data.form.fields.password.sublabel }}</span>

			<input class="o-form-create-account__input" id="{{ data.form.fields.password.id }}" name="{{ data.form.fields.password.name }}" type="{{ data.form.fields.password.options.type }}" data-parsley-error-message="{{ data.form.fields.password.errorMsg }}" data-parsley-errors-container=".o-form-create-account__errors-container" minlength="{{ data.form.fields.password.options.minlength }}" title="{{ data.form.fields.password.options.title }}" required/>
		</div>
	</div>
	<div class="o-form-create-account__errors-container"></div>

	<div class="obj-form__field">
		<label class="o-form-create-account__label-checkbox" for="{{ data.form.fields.formCheckAgreement.id }}">
			<input class="o-form-create-account__input-hidden" id="{{ data.form.fields.formCheckAgreement.id }}" type="checkbox" name="{{ data.form.fields.formCheckAgreement.name }}">
			<div class="o-form-create-account__icon-checked">
				{% include '@a-svg-icon' with {
					data: {
						name: 'checkbox-checked'
					},
						modifiers: ['medium', 'stroked-thin']
					}
				%}
			</div>
			<div class="o-form-create-account__icon-unchecked">
				{% include '@a-svg-icon' with {
					data: {
						name: 'checkbox-unchecked'
					},
						modifiers: ['medium', 'stroked-thin']
					}
				%}
			</div>
			<span class="o-form-create-account__label-text-checkbox">{{ data.form.fields.formCheckAgreement.label | raw}}</span>
		</label>
	</div>
</div>
