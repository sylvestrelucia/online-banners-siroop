<div class="o-profile-navigation obj-layout-6-3-3-3">
	{% for navItem in data.navItems %}
		<div class="obj-layout__col">
			<button class="o-profile-navigation__button {% if navItem.isActive %} is-active{% else %}is-inactive{% endif %}">
				<figure>
					<img class="o-profile-navigation__image" src="{{ navItem.image }}" alt="">
					<figcaption class="o-profile-navigation__text">{{ navItem.text }}</figcaption>
				</figure>
			</button>
		</div>
	{% endfor %}
</div>
