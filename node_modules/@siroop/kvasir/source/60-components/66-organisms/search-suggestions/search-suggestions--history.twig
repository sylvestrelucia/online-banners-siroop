<div class="o-search-suggestions{% if modifier %} o-search-suggestions--{{modifier}}{% endif %}" data-c-name="SearchSuggestions">
	<span class="o-search-suggestions__label">{{ data.description }}</span>
	{% for query in data.recentQueries %}
		<a class="o-search-suggestions__history-item" href="{{ query.url }}">{{ query.title }}</a>
	{% endfor %}
</div>
