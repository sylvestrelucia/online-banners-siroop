<div class="o-form-contact obj-form" data-c-name="FormContact">
	<ul class="o-form-contact__list">
		<li class="o-form-contact__field obj-form__field">
			<div class="obj-form obj-form__layout-6">
				<div class="o-form-contact__field obj-form__col">
					{% include '@m-form-field-input' with {
						data: data.form.firstName
					} %}
				</div>
				<div class="o-form-contact__field obj-form__col">
					{% include '@m-form-field-input' with {
						data: data.form.lastName
					} %}
				</div>
			</div>
		</li>
		<li class="o-form-contact__field obj-form__field obj-form__row">
			{% include '@m-form-field-input' with {
				data: data.form.lastName
			} %}
		</li>
		<li class="o-form-contact__field obj-form__field obj-form__row">
			{% include '@m-form-field-input' with {
				data: data.form.message
			} %}
		</li>
		<li class="o-form-contact__field obj-form__field">
			<input class="o-form-contact__button-submit obj-form__button-submit" type="submit" value="Abschicken">
		</li>
	</ul>
</div>
