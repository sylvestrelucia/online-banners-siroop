<ul class="m-social-links">
	{% for link in data.links %}
		<li class="m-social-links__icon">
			<a class="m-social-links__link m-social-links__link--{{ link.icon }}" href="{{ link.url }}"
			   target="_blank"{% if link.tooltip %} data-tooltip="{{ link.title }}"{% endif %} data-app-url="{{ link.app_url }}" aria-label="{{ link.title }}" rel="noopener">
				{% include '@a-svg-icon' with {
					modifiers: ['filled', 'large'],
					data: {
						name: link.icon
					}
				} %}
			</a>
		</li>
	{% endfor %}
</ul>
