<div class="m-energy-label{% if modifier %} m-energy-label--{{ modifier }}{% endif %}" data-c-name="EnergyLabel">
	<div class="m-energy-label__wrapper">
		<span class="m-energy-label__text">{{ data.text | raw }}<sup class="m-energy-label__sup">{{ data.sup | raw }}</sup></span>
	</div>
	{% if data.desc %}<span class="m-energy-label__link"><a href="{{ data.desc.url }}">{{ data.desc.link }}</a></span>{% endif %}
</div>
