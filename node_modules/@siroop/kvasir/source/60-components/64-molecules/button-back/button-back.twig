<button class="m-button-back{% if modifier %} m-button-back--{{ modifier }}{% endif %}">
	<i class="m-button-back__icon">
		{% include '@a-svg-icon' with {
			modifiers: ['stroked'],
			data: {
				name: 'arrow-long-right'
			}
		} %}
	</i>
	<span class="m-button-back__text">
		{{ data.text | raw}}
	</span>
</button>
