<div class="m-voucher-form" data-c-name="VoucherForm">

	<div class="m-voucher-form__title">
		{{ data.title }}
	</div>

	<div class="m-voucher-form__form">
		<div class="m-voucher-form__form-input">
			<div class="m-voucher-form__input-voucher">
				{% if data.customInput %}
					{{ data.customInput | raw }}
				{% endif %}
			</div>
		</div>
		<div class="m-voucher-form__form-button">
			<div class="m-voucher-form__button-submit">
				{% if data.customButton %}
					{{ data.customButton | raw }}
				{% endif %}
			</div>
		</div>
	</div>
</div>
