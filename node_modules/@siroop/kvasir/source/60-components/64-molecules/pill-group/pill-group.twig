{% spaceless %}
	<div href="#" class="m-pill-group{% if data.modifier %} m-pill-group--{{ data.modifier }} {% endif %}">
		{% for pill in data.pills %}
			<div class="a-pill{% if pill.modifier %} a-pill--{{ pill.modifier }}{% endif %} a-pill--{{ data.size }}">{{ pill.text }}</div>
		{% endfor %}
	</div>
{% endspaceless %}
