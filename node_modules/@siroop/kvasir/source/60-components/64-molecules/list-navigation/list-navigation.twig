<div class="m-list-navigation{% if modifier %} m-list-navigation--{{ modifier }}{% endif %}">
	<ul class="m-list-navigation__list">
		{% for item in data.items %}
			<li class="m-list-navigation__item{% if item.active %} m-list-navigation__item--active{% endif %}">
				<a href="{{ item.url }}" class="m-list-navigation__link">{{ item.name }}</a>
			</li>
		{% endfor %}
	</ul>
</div>
