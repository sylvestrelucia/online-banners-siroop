<div class="m-back-link{% if modifier %} m-back-link--{{ modifier }}{% endif %}">
	<a class="m-back-link__link" href={{ data.url }} title="{{ data.text|striptags }}">
			<span class="m-back-link__item-arrow">
				{% include '@a-svg-icon' with {
				modifiers: ['stroked', 'medium'], data: { name: 'arrow-left' }
				} %}
			</span>
		<span property="name">{{ data.text|raw }}</span>
	</a>
</div>
