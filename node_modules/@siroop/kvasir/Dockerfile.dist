FROM busybox

RUN mkdir -p /staticfiles/js
RUN mkdir -p /staticfiles/css
COPY ./public/assets/js/default.js /staticfiles/js/kvasir-default.min.js
COPY ./public/assets/js/styleguide.js /staticfiles/js/kvasir-styleguide.min.js
COPY ./public/assets/js/vendor.js /staticfiles/js/kvasir-vendor.min.js

COPY ./public/assets/css/default.css /staticfiles/css/kvasir-default.min.css
